{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApiContext": "Server=(localdb)\\mssqllocaldb;Database=CityInformation;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AppSettings": {
    "OpenweatherAppId": "<your Openweather API AppId>",
    "OpenweatherApiUrl": "https://api.openweathermap.org/data/2.5/weather?q={0}&APPID={1}",
    "CountryApiUrl": "https://restcountries.eu/rest/v2/name/{0}"
  } 
}
